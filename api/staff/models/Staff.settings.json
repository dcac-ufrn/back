{
  "connection": "default",
  "collectionName": "staff",
  "info": {
    "name": "staff",
    "description": "Pessoal administrativo das divisões administrativas, acadêmicas e científicas."
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "nome_completo": {
      "required": true,
      "type": "string"
    },
    "division": {
      "model": "division"
    },
    "vinculo": {
      "required": true,
      "enum": [
        "nao_definido",
        "estatutario",
        "bolsista",
        "clt_fund_apoio",
        "clt",
        "prest_serv_pfisica",
        "prest_serv_pj"
      ],
      "type": "enumeration"
    },
    "cargo": {
      "required": true,
      "enum": [
        "assistente_em_administracao",
        "auxiliar_em_administracao",
        "apoio_tecnico_administrativo",
        "outro"
      ],
      "enumDefault": "outro",
      "type": "enumeration"
    },
    "avatar": {
      "model": "avatar"
    }
  }
}