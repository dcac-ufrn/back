{
  "connection": "default",
  "collectionName": "avatar",
  "info": {
    "name": "avatar",
    "description": "Fotos dos docentes (DCAC ou PPGCC) e pesquisadores PNPD"
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "imagem_upada": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "nome_pessoa_numero": {
      "required": true,
      "type": "string"
    },
    "origem": {
      "enum": [
        "sigaa",
        "lattes",
        "publons",
        "researchgate",
        "linkedin",
        "entrega_pelo_professor_ou_pesquisador",
        "foto_pela_equipe_administrativa",
        "outra"
      ],
      "type": "enumeration",
      "required": true
    },
    "link": {
      "type": "string"
    },
    "Publicar": {
      "required": true,
      "type": "boolean"
    }
  }
}