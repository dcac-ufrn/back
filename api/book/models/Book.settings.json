{
  "connection": "default",
  "collectionName": "book",
  "info": {
    "name": "book",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "isbn": {
      "type": "string"
    },
    "titulo": {
      "required": true,
      "type": "string"
    },
    "subtitulo": {
      "type": "string"
    },
    "responsabilidade_intelectual": {
      "required": true,
      "enum": [
        "nao_especificada",
        "autor",
        "organizador",
        "compilador",
        "editor",
        "coordenador"
      ],
      "type": "enumeration"
    },
    "numero_edicao": {
      "type": "integer",
      "required": true
    },
    "ano_edicao": {
      "required": true,
      "type": "string"
    },
    "quantidade_paginas": {
      "type": "integer"
    },
    "nome_serie": {
      "type": "string"
    },
    "traducao_por": {
      "type": "string"
    },
    "titulo_original_se_em_outro_idioma": {
      "type": "string"
    },
    "online": {
      "required": true,
      "type": "boolean"
    },
    "url": {
      "type": "string"
    },
    "idioma_edicao": {
      "required": true,
      "enum": [
        "nao_especificado",
        "portugues",
        "espanhol",
        "italiano",
        "frances",
        "ingles",
        "alemao",
        "outro"
      ],
      "type": "enumeration"
    },
    "publishers": {
      "collection": "publisher"
    },
    "authors": {
      "collection": "person"
    },
    "tipo_autoria": {
      "required": true,
      "enum": [
        "pessoa_fisica",
        "ent_priv_coletiv_c_ou_s_fins_lucrat",
        "br_gov",
        "br_organiz_pub",
        "pais_estrang_gov",
        "pais_estrang_organiz_pub",
        "desconhecida"
      ],
      "type": "enumeration"
    },
    "adaptacao_de_outra_obra": {
      "required": true,
      "type": "boolean"
    },
    "tipo_orig_cont_obra": {
      "enum": [
        "propria_pesq_ou_desenv_pelo_responsavel_obra",
        "entrevista",
        "depoimento",
        "declaracao",
        "psicografia"
      ],
      "type": "enumeration",
      "required": true
    },
    "nome_orig_conteudo_obra_se_nao_propria": {
      "type": "string"
    },
    "nome_autor_obra_adaptada": {
      "type": "string"
    }
  }
}